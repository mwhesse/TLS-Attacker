        <alias>server</alias>
        <ip>localhost</ip>
        <port>443</port>
        <hostname>localhost</hostname>
        <timeout>1000</timeout>
        <connectionTimeout>8000</connectionTimeout>
        <transportHandlerType>TCP</transportHandlerType>
        <useIpv6>false</useIpv6>
    </InboundConnection>
    <OutboundConnection>
        <alias>client</alias>
        <ip>localhost</ip>
        <port>443</port>
        <hostname>localhost</hostname>
        <timeout>1000</timeout>
        <connectionTimeout>8000</connectionTimeout>
        <transportHandlerType>TCP</transportHandlerType>
